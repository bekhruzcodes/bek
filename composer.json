{
    "name": "bekhruz/bek",
    "description": "A simple version control system",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bekhruz\\Bek\\": "src/"
        }
    },
    "bin": ["bin/bek"],
    "scripts": {
        "post-install-cmd": [
            "chmod +x bin/bek",
            "php vendor/bekhruz/bek/install.php"
        ],
        "post-update-cmd": [
            "chmod +x bin/bek",
            "php vendor/bekhruz/bek/install.php"
        ]
    },
    "authors": [
        {
            "name": "bekhruzcodes",
            "email": "bekhruzbekmirzaliev744@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}